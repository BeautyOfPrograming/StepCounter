<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:dist="http://schemas.android.com/apk/distribution"
    package="com.example.SaberiGhdamShomar">

    <dist:module dist:instant="true" />

    <application
        android:name="com.example.SaberiGhdamShomar.TapselClass"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_foreground"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_foreground"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.SaberiGhdamShomar.ScreenSlidePagerActivity"></activity>
        <activity android:name="com.example.SaberiGhdamShomar.settings" />
        <activity android:name="com.example.SaberiGhdamShomar.NewWordActivity" />

        <activity android:name="ir.tapsell.sdk.TapsellAdActivity"
            android:configChanges="keyboardHidden|orientation|screenSize" >
        </activity>

        <activity
            android:name="com.example.SaberiGhdamShomar.DialogAlert"
            android:theme="@style/Theme.Transparent.AppTheme.picker" />
        <activity
            android:name="com.example.SaberiGhdamShomar.ResultDialog"
            android:theme="@style/Theme.Transparent.AppTheme.picker" />
        <activity
            android:name="com.example.SaberiGhdamShomar.MainActivity"
            android:windowSoftInputMode="stateHidden">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name="com.example.SaberiGhdamShomar.SoundService" />




    </application>


</manifest>